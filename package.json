{
  "name": "type-bot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "dev": "node --loader ts-node/esm/transpile-only src/main.ts",
